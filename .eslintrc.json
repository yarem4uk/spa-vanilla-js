{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-unused-vars": ["warn", { "vars": "local", "args": "none" }],
    "no-console": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off"
  }
}
